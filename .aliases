# Environment Aliases

alias upbrew="brew -v update; brew upgrade --force-bottle; brew cleanup; brew doctor"
alias upnpm="npm-check -g -u; npm install npm -g; npm update -g"
alias upgem="sudo gem update --system; sudo gem update -n/usr/local/bin --no-document"
alias upd="upbrew; upnpm; upgem"
